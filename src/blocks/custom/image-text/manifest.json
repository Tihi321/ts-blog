{
  "blockName": "image-text",
  "title": "Image Text",
  "description" : "Image Text with custom settings.",
  "category": "common",
  "icon": {
    "src": "vault"
  },
  "keywords": [
    "Image",
    "Image Text",
    "Block Image"
  ],
  "attributes": {
    "mediaId": {
      "type": "number",
      "manualAction": true
    },
    "mediaUrl": {
      "type": "string",
      "default": "https://via.placeholder.com/500",
      "manualAction": true
    },
    "mediaType": {
      "type": "string",
      "default": "image"
    },
    "mediaSize": {
      "type": "string",
      "default": "large"
    },
    "heading": {
      "type": "string"
    },
    "paragraph": {
      "type": "string"
    },
    "imagePosition": {
      "type": "string",
      "default": "left"
    }
  }
}
